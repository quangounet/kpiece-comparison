<Environment>
  <Robot file="../../robots/densowithtray.xml" name="Densuke"></Robot>

  <Kinbody name="floor">
    <Body type="static">
      <Geom type="box">
	<extents>2 2 0.05</extents>
	<Translation>0 0 -0.05</Translation>
      </Geom>
    </Body>
  </Kinbody>

  <Kinbody name="box1">
    <!-- side wall -->
    <Body type="static">
      <Geom type="box">
	<extents>0.05 0.3 0.4</extents>
	<diffusecolor>1.00 0.2745 0.1608</diffusecolor>
      </Geom>
    </Body>
  </Kinbody>

  <Kinbody name="box2">
    <!-- upper edge of the opening -->
    <Body type="static">
      <Geom type="box">
	<extents>0.05 0.715 0.2</extents>
	<diffusecolor>1.00 0.2745 0.1608</diffusecolor>
      </Geom>
    </Body>
  </Kinbody>

  <Kinbody name="box3">
    <!-- lower edge of the opening -->
    <Body type="static">
      <Geom type="box">
	<extents>0.05 0.414 0.16</extents>
	<diffusecolor>1.00 0.2745 0.1608</diffusecolor>
      </Geom>
    </Body>
  </Kinbody>

  <Kinbody name="box4">
    <Body type="static">
      <Geom type="box">
	<extents>0.05 0.329 0.15</extents>
	<diffusecolor>1.00 0.2745 0.1608</diffusecolor>
      </Geom>
    </Body>
  </Kinbody>

  <Kinbody name="box5">
    <!-- side edge of the opening -->
    <Body type="static">
      <Geom type="box">
	<extents>0.05 0.124 0.4</extents>
	<diffusecolor>1.00 0.2745 0.1608</diffusecolor>
      </Geom>
    </Body>
  </Kinbody>

  <Kinbody name="bottle">
    <body name="bottle_lowerpart">
      <Geom type="box">
	<extents>0.04 0.04 0.08</extents>
	<diffusecolor>0.616 0.847 0.855</diffusecolor>
	<Translation>0.00 0.00 0.08</Translation>
      </Geom>
      <mass type="mimicgeom">
	<total>2.5</total>
      </mass>
    </body>

    <body name="bottle_upperpart">
      <offsetfrom>bottle_lowerpart</offsetfrom>
      <Geom type="box">
	<extents>0.04 0.04 0.04</extents>
	<Translation>0.00 0.00 0.20</Translation>
	<diffusecolor>0.616 0.847 0.855</diffusecolor>
	<transparency>0.4</transparency>
      </Geom>
      <mass type="mimicgeom">
	<total>0.0</total>
      </mass>
    </body>
    
    <adjacent>bottle_lowerpart bottle_upperpart</adjacent>
    <joint name="dummy3" type="hinge" enable="false">
      <body>bottle_lowerpart</body>
      <body>bottle_upperpart</body>
      <limits>0 0</limits>
    </joint>
  </Kinbody>
  
</Environment>